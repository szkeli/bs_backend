}

type Sentiment {
  # 正面分数
  positive
  # 中性分数
  neutral
  # 负面分数
  negative
  # 文本情感
  value
}

# 用户收到的通知 
type Notification {
  # 通知创建者
  creator
  # 通知的创建时间
  createdAt
  # 被通知的对象
  to
  # 通知的负载（比如 A用户点赞了B帖子 B帖子即为负载
  about
  # 枚举值，什么操作触发的通知（点赞 评论 关注 系统通知
  action
  # 通知接收者是否已读该通知
  isRead
}

# 置顶
type Pin {
  # 创建时间
