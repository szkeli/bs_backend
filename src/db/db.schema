}

# 用户在blackboard上的ddl
type Deadline {
  # deadline 唯一 id
  deadlineId
  # 创建时间
  createdAt
  # deadline 对应的课程的名称
  courseName
  # deadline 的开始时间
  startDate
  # deadline 的结束时间
  endDate
  # deadline 的标题
  title
  # deadline 所在的课程
  lesson
  # deadline 的类型 [用户创建、从内部网自动导入]
  type
}

# 帖子和评论的评论
type Comment {
  # 评论的创建时间
  createdAt
  # 评论的内容
  content
  # 评论的创建者
  creator
  # 评论的点赞
  votes
  # 评论收到的举报
  reports
  # 评论的评论
  comments
  # 被评论的对象
  to
  # 评论收到的折叠
