# 管理员
type Admin {
  # 唯一用户 id
  userId
  # 密码
  sign
  # 名字
  name
  # 头像链接
  avatarImageUrl
  # 注册时间
  createdAt
  # 用户信息更新时间
  updatedAt
  # 上一次登录时间
  lastLoginedAt
  # 当前管理员的认证凭证
  credential
  # 当前管理员对外分发的授权凭证
  credentials
  # 当前管理员的权限
  privileges
  # 删除 (删除用户 帖子 评论)
  deletes
  # 该管理员折叠的评论 (折叠评论)
  folds
  # 该管理员拉黑的用户 (拉黑用户)
  blocks
  # 当前管理员创建的置顶
  pins
}

# 用户
type User {
  # 唯一用户 id
  userId
  # 用户的密码
  sign
  # 用户的名字
  name
  # 用户的头像链接
  avatarImageUrl
  # 用户性别
  gender
  # 用户学号
  studentId
  # 用户的学校
  school
  # 用户所在的校区
  subCampus
  # 用户所在的学院
  college
  # 用户的年级
  grade
  # 用户的openId
  openId
  # 用户在白板小程序内的openId
  blankspaceOpenId
  # 用户在白板助手小程序内的openId
  blankspaceAssistantOpenId
  # 用户的unionId
  unionId
  # 用户注册时间
  createdAt
  # 用户上一次更新信息时间
  updatedAt
  # 用户上一次登录时间
  lastLoginedAt
  # 用户发布的帖子
  posts
  # 用户创建的主题 用于存放帖子
  subjects
  # 用户的点赞
  votes
  # 用户创建的会话
  conversations
  # 收到的举报
  reports
  # 用户的删除 (删除帖子 评论 主题)
  deletes
  # 用户被拉黑
  block
  # 当前用户的权限
  privileges
  # 用户的ddl
  deadlines
  # 当前用户的浏览
  views
  # 用户收到的通知
  notifications
  # 当前用户的认证凭证
  credential
  # 当前用户的角色
  roles
  # 当前用户的课程
  lessons
  # 课程的通知设置
  lessonNotificationSettings
  # 课程通知的通知状态
  lessonNotificationStatus
}

# 帖子
type Post {
  # 帖子的内容
  content
  # 帖子的图片
  images
  # 帖子的图片
  imagesV2
  # 帖子的创建者
  creator
  # 帖子的创建时间
